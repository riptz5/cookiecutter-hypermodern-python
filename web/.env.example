# Autopoietic Dashboard Environment Variables
# Copy this file to .env.local and fill in your values

# API Configuration
NEXT_PUBLIC_API_URL=http://localhost:8080

# Google Cloud Configuration
NEXT_PUBLIC_GCP_PROJECT_ID=your-gcp-project-id

# Development only
NEXT_PUBLIC_DEBUG_MODE=false
NEXT_PUBLIC_MOCK_DATA=true

# Feature Flags
NEXT_PUBLIC_ENABLE_REAL_TIME_LOGS=true
NEXT_PUBLIC_ENABLE_COST_TRACKING=true
NEXT_PUBLIC_ENABLE_AUTO_REFRESH=true
